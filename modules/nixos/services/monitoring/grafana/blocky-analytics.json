          },
          "editorMode": "code",
          "format": "table",
          "rawQuery": true,
          "rawSql": "WITH health_metrics AS (\n  SELECT\n    -- Error rate (lower is better, max 25 points)\n    LEAST(25, GREATEST(0, 25 - (COUNT(CASE WHEN response_code != 'NOERROR' THEN 1 END) * 100.0 / COUNT(*)) * 2.5)) as error_score,\n    -- Response time score (lower is better, max 25 points)\n    LEAST(25, GREATEST(0, 25 - (AVG(COALESCE(duration_ms, 0)) / 10.0))) as response_score,\n    -- Cache hit rate (higher is better, max 25 points)\n    (COUNT(CASE WHEN response_type = 'CACHED' THEN 1 END) * 25.0 / COUNT(*)) as cache_score,\n    -- Block effectiveness (higher is better, max 25 points)\n    LEAST(25, (COUNT(CASE WHEN response_type = 'BLOCKED' THEN 1 END) * 25.0 / COUNT(*))) as block_score\n  FROM log_entries\n  WHERE $__timeFilter(request_ts)\n)\nSELECT \n  ROUND((error_score + response_score + cache_score + block_score)::numeric, 1) as value\nFROM health_metrics",
          "refId": "A"
        }
      ],
      "title": "DNS Query Health Score",
      "type": "stat"
    }
