          "rawSql": "SELECT \n  CASE \n    WHEN reason LIKE '%ads%' AND reason LIKE '%trackers%' THEN 'ads,trackers'\n    WHEN reason LIKE '%ads%' THEN 'ads'\n    WHEN reason LIKE '%trackers%' THEN 'trackers'\n    WHEN reason LIKE '%BLOCKED%' THEN 'other blocking'\n    WHEN reason LIKE '%CACHED%' THEN 'cached'\n    WHEN reason LIKE '%RESOLVED%' THEN 'resolved'\n    ELSE 'other'\n  END as blocking_category,\n  count(*) as value\nFROM log_entries \nWHERE $__timeFilter(request_ts)\nGROUP BY \n  CASE \n    WHEN reason LIKE '%ads%' AND reason LIKE '%trackers%' THEN 'ads,trackers'\n    WHEN reason LIKE '%ads%' THEN 'ads'\n    WHEN reason LIKE '%trackers%' THEN 'trackers'\n    WHEN reason LIKE '%BLOCKED%' THEN 'other blocking'\n    WHEN reason LIKE '%CACHED%' THEN 'cached'\n    WHEN reason LIKE '%RESOLVED%' THEN 'resolved'\n    ELSE 'other'\n  END\nORDER BY value DESC",
